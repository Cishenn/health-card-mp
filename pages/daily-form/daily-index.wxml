<!--pages/daily-form/daily-index.wxml-->
<no-group wx:if="{{hasGroup==false}}">
</no-group>
<community wx:elif="{{hasGroup==true&&role=='community'}}"
hasSubmit="{{hasSubmit}}">
</community>
<school wx:elif="{{hasGroup==true&&role=='school'}}"
hasSubmit="{{hasSubmit}}">
</school>
<other wx:elif="{{hasGroup==true&&role=='other'}}"
hasSubmit="{{hasSubmit}}">
</other>
